{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"  
      }
    }
  ],
  "routes": [
    {
      "src": "/index.html",  
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1" 
    }
  ]
}
